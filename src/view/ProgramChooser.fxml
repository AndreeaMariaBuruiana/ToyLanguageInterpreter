<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="view.ProgramListController" prefWidth="600" prefHeight="400">
    <Label text="Select a Program to Execute" style="-fx-font-weight: bold; -fx-font-size: 16px; -fx-padding: 10;"/>
    <ListView fx:id="programListView" VBox.vgrow="ALWAYS" />
    <Button text="Run Selected Program" onAction="#handleRunButton" prefWidth="200" style="-fx-background-color: #4CAF50; -fx-text-fill: white; -fx-margin-bottom: 10;"/>
</VBox>